<Page x:Class="slv_authApp.Screens.Auth"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:slv_authApp.Screens"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Auth">

    <StackPanel>
        <Label Margin="1 1 1 1" Content="Login"></Label>
        <TextBox Margin="1 1 1 1" x:Name="Login"></TextBox>
        <Label Margin="1 1 1 1" Content="Password"></Label>
        <PasswordBox Margin="1 1 1 1" x:Name="Password"></PasswordBox>
        <Button Margin="1 1 1 1" Content="Enter" Click="Button_Click"></Button>
        <Button Margin="1 1 1 1" Content="Registration" Click="Button_Click_1"></Button>
        
    </StackPanel>
</Page>
